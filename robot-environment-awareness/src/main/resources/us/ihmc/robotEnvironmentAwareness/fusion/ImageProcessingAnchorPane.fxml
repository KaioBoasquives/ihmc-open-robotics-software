<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="166.0" prefWidth="575.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="us.ihmc.robotEnvironmentAwareness.fusion.controller.ImageProcessingAnchorPaneController">
   <children>
      <VBox alignment="CENTER" layoutX="20.0" layoutY="19.0" prefHeight="130.0" prefWidth="137.0" spacing="5.0">
         <children>
            <ToggleButton fx:id="btnEnableStreaming" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Enable Stream" />
            <Button fx:id="btnTaskingSnapshot" maxWidth="1.7976931348623157E308" mnemonicParsing="false" prefHeight="25.0" prefWidth="71.0" text="Take SnapShot" />
            <Button fx:id="btnClearImageView" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#clearImageView" prefHeight="25.0" prefWidth="71.0" text="Clear" />
         </children>
      </VBox>
      <Button layoutX="196.0" layoutY="41.0" mnemonicParsing="false" onAction="#load" prefHeight="25.0" prefWidth="147.0" text="Load" />
      <Button layoutX="196.0" layoutY="72.0" mnemonicParsing="false" onAction="#showProjection" prefHeight="25.0" prefWidth="147.0" text="Show Projection" />
      <Button layoutX="196.0" layoutY="105.0" mnemonicParsing="false" onAction="#showFilteredProjection" prefHeight="25.0" prefWidth="147.0" text="Show Filtered Projection" />
      <Button layoutX="357.0" layoutY="105.0" mnemonicParsing="false" onAction="#showFilteredPoints" prefHeight="25.0" prefWidth="147.0" text="Show Filtered Points" />
   </children>
</AnchorPane>
